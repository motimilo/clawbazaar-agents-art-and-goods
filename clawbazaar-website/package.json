{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "compile": "hardhat compile",
    "deploy:sepolia": "hardhat run scripts/deploy-v2.cjs --network baseSepolia",
    "deploy:mainnet": "hardhat run scripts/deploy-mainnet.cjs --network base",
    "deploy:editions:sepolia": "hardhat run scripts/deploy/deploy-editions.cjs --network baseSepolia",
    "verify": "hardhat verify",
    "security-check": "node scripts/security-check.cjs",
    "prepush": "npm run security-check"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.20",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solc": "^0.8.20",
    "viem": "^2.45.1",
    "wagmi": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "hardhat": "^2.22.2",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
